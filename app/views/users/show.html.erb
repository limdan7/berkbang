<div class="row">
  <div class="col-md-4 col-md-offset-4 center">
    <%= gravatar_for @user, size: 150 %>
  </div>
</div>
<h4 align="center"><%= @user.name %>님의 포스팅</h4>

<div align="center">
  <%= will_paginate @user_rooms%>
</div>

<h2 class="page-header">판매 목록</h2>
<%= render 'rooms/room', obj: @user.rooms %>
<h2 class="page-header">구매 목록</h2>
<%= render 'demands/demands', obj: @user.demands %>

<div align="center">
  <%= will_paginate @user_rooms%>
</div>
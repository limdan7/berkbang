<h1 align="center">전체 카테고리</h1>
<div class="center">
<%= will_paginate %>
  <% @categories.each do |category| %>
    <ul class="listing">
      <div class="row">
        <div class="well col-md-4 col-md-offset-4">
          <li class="room-title">
            <%= link_to category.name, category_path(category) %>
          </li>
          <li><small><%= pluralize(category.rooms.count,"room") %></small></li>
        </div>
      </div>
    </ul>
  <% end %>
<%= will_paginate %>
</div>